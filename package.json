{
    "homepage": "https://sill.etalab.gouv.fr",
    "name": "sill-web",
    "version": "0.26.6",
    "license": "MIT",
    "scripts": {
        "start": "./.env.local.sh react-app-rewired start",
        "build": "react-app-rewired build",
        "keycloak": "yarn build && keycloakify",
        "storybook": "start-storybook -p 6006",
        "postinstall": "generate-env-getter",
        "prestart": "generate-env-getter",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "npm run _format -- --write",
        "format:check": "npm run _format -- --list-different",
        "link_api": "ts-node --skip-project src/bin/link_inhouse_deps.ts sill-api",
        "analyze": "source-map-explorer 'build/static/js/*.js'"
    },
    "devDependencies": {
        "@emotion/css": "^11.10.0",
        "@storybook/addon-actions": "^6.5.10",
        "@storybook/addon-essentials": "^6.5.10",
        "@storybook/addon-links": "^6.5.10",
        "@storybook/builder-webpack5": "^6.5.0-rc.1",
        "@storybook/manager-webpack5": "^6.5.0-rc.1",
        "@storybook/node-logger": "^6.5.10",
        "@storybook/preset-create-react-app": "^4.1.2",
        "@storybook/react": "^6.5.10",
        "@types/express": "4.17.13",
        "@types/jwt-simple": "^0.5.33",
        "@types/memoizee": "^0.4.4",
        "@types/node": "^14.0.13",
        "@types/react": "^18.0.18",
        "@types/react-dom": "^18.0.6",
        "@types/react-redux": "^7.1.12",
        "@typescript-eslint/eslint-plugin": "^4.24.0",
        "@typescript-eslint/parser": "^4.24.0",
        "circular-dependency-plugin": "^5.2.2",
        "crypto": "file:./.storybook/.compatibility/crypto",
        "csv-parse": "^5.0.4",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-tss-unused-classes": "^0.0.4",
        "events": "^3.2.0",
        "github-pages-plugin-for-type-route": "^0.1.0",
        "husky": "^4.3.8",
        "lint-staged": "^11.0.0",
        "minimal-polyfills": "^2.2.2",
        "prettier": "^2.3.0",
        "react-app-rewired": "^2.2.1",
        "source-map-explorer": "^2.5.2",
        "ts-node": "^10.2.1",
        "typescript": "^4.8.2"
    },
    "dependencies": {
        "@emotion/react": "^11.4.1",
        "@emotion/styled": "^11.3.0",
        "@mui/icons-material": "^5.0.4",
        "@mui/material": "^5.2.5",
        "@reduxjs/toolkit": "^1.4.0",
        "@trpc/client": "^9.24.0",
        "buffer": "^6.0.3",
        "comment-json": "^3.0.0",
        "cra-envs": "^1.2.8",
        "evt": "^2.4.4",
        "i18nifty": "^1.3.17",
        "jwt-simple": "^0.5.6",
        "keycloak-js": "^18.0.0",
        "keycloakify": "^6.5.0",
        "memoizee": "^0.4.14",
        "moment": "^2.29.1",
        "node-fetch": "^2.6.1",
        "onyxia-ui": "^0.47.1",
        "path": "^0.12.7",
        "powerhooks": "^0.20.20",
        "react": "^18.2.0",
        "react-async-hook": "^3.6.1",
        "react-dom": "^18.2.0",
        "react-markdown": "^5.0.3",
        "react-query": "4.0.0-beta.23",
        "react-redux": "^7.2.1",
        "react-scripts": "5.0.1",
        "redux-clean-architecture": "^1.2.12",
        "run-exclusive": "^2.2.16",
        "sill-api": "0.24.0",
        "tsafe": "^1.1.1",
        "tss-react": "^4.3.4",
        "type-route": "^0.7.1",
        "unorm": "^1.6.0",
        "url-join-ts": "^1.0.5",
        "zod": "^3.17.3"
    },
    "resolutions": {
        "powerhooks": "0.20.20"
    },
    "eslintConfig": {
        "plugins": [
            "tss-unused-classes"
        ],
        "extends": [
            "react-app",
            "prettier"
        ],
        "rules": {
            "@typescript-eslint/no-redeclare": "off",
            "import/no-anonymous-default-export": "off",
            "react-hooks/rules-of-hooks": "warn",
            "react-hooks/exhaustive-deps": "off",
            "no-labels": "off",
            "prefer-const": "off",
            "no-lone-blocks": "off",
            "tss-unused-classes/unused-classes": "warn"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "lint-staged": {
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
